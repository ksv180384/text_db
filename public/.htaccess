<IfModule mod_rewrite.c>
#    Options -MultiViews

    RewriteEngine On

    RewriteBase /BeeJee_php/public
#    RewriteCond %{REQUEST_FILENAME} !-d
#    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond $1 !^(index\.php|robots\.txt|js|css)
    RewriteRule ^(.*)$ /index.php?path=$1 [L,QSA]
</IfModule>
